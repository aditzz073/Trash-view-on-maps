{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "proxy/proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "server/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/proxy/(.*)",
      "dest": "/proxy/proxy.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/server.js"
    }
  ]
}
